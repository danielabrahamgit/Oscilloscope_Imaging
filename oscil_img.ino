void setup() 
{
	//Necessary to set up the clock frequency used	
	rtc_clk_cpu_freq_set(RTC_CPU_FREQ_240M);
	//Enable both DACs
	dac_output_enable(DAC_CHANNEL_1);
	dac_output_enable(DAC_CHANNEL_2);
}

void loop() 
{
	//Turn on the DACS
	DACPrepare(true);
	//Index through every row(y) and every column(x)
	for (int y = 0; y < Y_RES; y++)
	{
		for (int x = 0; x < X_RES; x++)
		{
			//This mimicks a constant scan from the bottom 
			//to the top of the image. 
			DAC2Write(255 * 1.0 * y / (Y_RES + 1));
			//Get grascale value
			int grey = pixel_map[Y_RES - 1 - y][x];
			//Write off of the screen (255) if black pixel
			if (grey <= BLACK)
				DAC1Write(255);
			else
			{
				//Go to the correct column
				DAC1Write(255 * 1.0 * x / (X_RES + 1));
				//Hault at pixel proportional to pixel brightness. 
				//Our eyes will see longer pixel exposure as a birghter pixel.
				delayMicroseconds(50 * 1.0*(grey - BLACK) / (255 - BLACK));
			}
		}
	}
	//Turn off the DACs
	DACUnprepare(true);
}
